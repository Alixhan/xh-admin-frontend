    "echarts": "^5.5.0",
    "element-plus": "^2.6.0",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "filesize": "^10.1.0",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
